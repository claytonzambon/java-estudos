package exercicios_OO;

public class MusicaTeste {

	public static void main(String[] args) {
		
		Musica musica = new Musica();
		musica.titulo = "Nine Lives";
		musica.artista = "Aerosmith";
		musica.anoLancamento = 1998;
		musica.avalia(9);
		musica.avalia(8);
		musica.avalia(7);
		System.out.println(musica.exibeFichaTecnica());

	}

}
